<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pantallaInicio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResumenActivity">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

    </android.support.design.widget.AppBarLayout>

    <TextView
        android:id="@+id/numPreg"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="21dp"
        android:text="@string/preguntas_disponibles_10" />

    <TextView
        android:id="@+id/numCat"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="54dp"
        android:text="@string/categor_as_disponibles_2" />

    <TextView
        android:id="@+id/testhechos"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="86dp"
        android:text="@string/cuestionarios_realizados_10" />

    <TextView
        android:id="@+id/calmedia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="113dp"
        android:text="@string/calificaci_n_media_6_7_10" />

    <Button
        android:id="@+id/anim"
        android:layout_width="118dp"
        android:layout_height="37dp"
        android:layout_alignStart="@+id/numPreg"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="123dp"
        android:layout_marginBottom="137dp"
        android:text="@string/anim" />

    <ImageView
        android:id="@+id/ivanim"
        android:layout_width="83dp"
        android:layout_height="86dp"
        android:layout_alignStart="@+id/numPreg"
        android:layout_alignParentTop="true"
        android:layout_marginStart="139dp"
        android:layout_marginTop="222dp"
        app:srcCompat="@drawable/app" />

    <TextView
        android:id="@+id/conexion"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-28dp"
        android:layout_marginBottom="37dp"
        android:layout_toEndOf="@+id/calmedia" />

</RelativeLayout>